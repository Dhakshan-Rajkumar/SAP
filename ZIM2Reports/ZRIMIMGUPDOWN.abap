*&      Form  P2000_UPLOAD
*&---------------------------------------------------------------------*
FORM P2000_UPLOAD TABLES   P_TAB
                  USING    P_TABNAME.

  DATA : L_DATE_FORMAT(10)  TYPE  C.
  CLEAR : W_FILENAME.

  SELECT SINGLE * FROM USR01 WHERE BNAME EQ SY-UNAME.
  CASE USR01-DATFM.
     WHEN '1'.
        L_DATE_FORMAT = 'DD.MM.YYYY'.
     WHEN '2'.
        L_DATE_FORMAT = 'MM/DD/YYYY'.
     WHEN '3'.
        L_DATE_FORMAT = 'MM-DD-YYYY'.
     WHEN '4'.
        L_DATE_FORMAT = 'YYYY.MM.DD'.
     WHEN '5'.
        L_DATE_FORMAT = 'YYYY/MM/DD'.
     WHEN '6'.
        L_DATE_FORMAT = 'YYYY-MM-DD'.
  ENDCASE.

  CONCATENATE 'C:\' P_TABNAME '.TXT' INTO W_FILENAME.
  CALL FUNCTION 'WS_UPLOAD'
    EXPORTING
      FILENAME = W_FILENAME
      FILETYPE = 'DAT'
      DAT_D_FORMAT = L_DATE_FORMAT
    TABLES
      DATA_TAB = P_TAB.

  IF SY-SUBRC NE 0.
     MESSAGE E463(ZIM1) WITH W_FILENAME.
  ENDIF.

ENDFORM.                    " P2000_UPLOAD
