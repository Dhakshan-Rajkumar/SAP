*&      Form  P1000_READ_TABLE
*&---------------------------------------------------------------------*
FORM P1000_READ_TABLE.

  REFRESH IT_TAB.
  CLEAR   IT_TAB.

* IMG 00~24.
  SELECT DISTINCT *
    INTO CORRESPONDING FIELDS OF TABLE IT_TAB
    FROM DD02T
   WHERE TABNAME    GE 'ZTIMIMG00'
     AND TABNAME    LE 'ZTIMIMG24'
     AND DDLANGUAGE EQ 'E'
     AND AS4LOCAL   EQ 'A'.

* Port Code
  SELECT    DISTINCT *
  APPENDING CORRESPONDING FIELDS OF TABLE IT_TAB
    FROM    DD02T
   WHERE    TABNAME    IN ('ZTIEPORT', 'ZTIMGTXT')
     AND    DDLANGUAGE EQ 'E'
     AND    AS4LOCAL   EQ 'A'.

ENDFORM.                    " P1000_READ_TABLE
