FORM P2000_DOWNLOAD TABLES   P_TAB
                    USING    P_TABNAME.

  CLEAR : W_FILENAME.

  CONCATENATE 'C:\' P_TABNAME '.txt' INTO W_FILENAME.
  CALL FUNCTION 'DOWNLOAD'
       EXPORTING
            FILENAME = W_FILENAME
            FILETYPE = 'DAT'
       TABLES
            DATA_TAB = P_TAB.

ENDFORM.                    " P2000_DOWNLOAD

