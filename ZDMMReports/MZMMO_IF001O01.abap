MODULE STATUS_0100 OUTPUT.
  set pf-status '100'.
  set titlebar '100'.

  if gv_first is initial.
    clear it_sub.
    refresh it_sub.
    select * into
    corresponding fields of table it_sub
    from ztmm_if002.

    gv_first = 'X'.
  endif.

ENDMODULE.                 " STATUS_0100  OUTPUT
